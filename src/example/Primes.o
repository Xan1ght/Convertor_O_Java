MODULE Primes;

IMPORT
   Out, In;

CONST
   w = 6;

VAR
   i, n, d: INTEGER;
   x, y, c: INTEGER;

BEGIN
(*
   In.Open;
   In.Int(n);
*)
   n := 1000;

   c := 0;
   i := 2;
   WHILE i <= n DO
      d := 2;
      WHILE i MOD d # 0 DO
          d := d + 1;
      END;
      IF d = i THEN
(*
         IF c MOD w = 0 THEN
            Out.Ln;
         END;
         Out.Int(i, w);
*)
         INC(c);
      END;
      i := i + 1;
   END;
   Out.Ln();
   Out.Int(c, 0);
   Out.Ln();

END Primes.

